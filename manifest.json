{
  "name": "Chrome new tab design",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["scripts/app.js"],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/*",
    "chrome://favicon/",
    "tabs",
    "<all_urls>"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'"
}
